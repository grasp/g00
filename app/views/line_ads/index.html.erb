<h1>Listing line_ads</h1>

<table>
  <tr>
    <th>Line</th>
    <th>Fcity name</th>
    <th>Tcity name</th>
    <th>Company name</th>
    <th>Url</th>
    <th>One word</th>
    <th>Contact phone</th>
    <th>User</th>
    <th>Company</th>
    <th>Lstatistic</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @line_ads.each do |line_ad| %>
  <tr>
    <td><%= line_ad.line %></td>
    <td><%= line_ad.fcity_name %></td>
    <td><%= line_ad.tcity_name %></td>
    <td><%= line_ad.company_name %></td>
    <td><%= line_ad.url %></td>
    <td><%= line_ad.one_word %></td>
    <td><%= line_ad.contact_phone %></td>
    <td><%= line_ad.user_id %></td>
    <td><%= line_ad.company_id %></td>
    <td><%= line_ad.lstatistic_id %></td>
    <td><%= link_to 'Show', line_ad %></td>
    <td><%= link_to 'Edit', edit_line_ad_path(line_ad) %></td>
    <td><%= link_to 'Destroy', line_ad, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Line ad', new_line_ad_path %>
